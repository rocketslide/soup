<!DOCTYPE html>
<html>
<head>
  <title>Soupbowl</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  
  <div id="logout">
    <% if session[:user_id] %>
    <%= button_to 'Logout', logout_path, :method => :delete	%>
    <% end %>
  </div>

<%= yield %>

</body>
</html>
